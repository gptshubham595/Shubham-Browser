
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:orientation="vertical"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:paddingLeft="@dimen/activity_horizontal_margin"
    android:paddingRight="@dimen/activity_horizontal_margin"
    android:paddingTop="@dimen/activity_vertical_margin"
    android:paddingBottom="@dimen/activity_vertical_margin"
    tools:context=".MyActivity"
    android:background="@drawable/g"
    >

    <EditText
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:inputType="textMultiLine"
        android:ems="10"
        android:id="@+id/url"
        android:layout_alignParentTop="true"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true"
        android:background="#ff46ff56"
        android:textColor="#ff171bff"
        android:hint="     Search or Enter Url"
        android:layout_alignBottom="@+id/search"
        android:layout_toLeftOf="@+id/search"
        android:layout_toStartOf="@+id/search" />

    <Button
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Search"
        android:id="@+id/search"
        android:layout_alignParentTop="true"
        android:layout_alignParentRight="true"
        android:layout_alignParentEnd="true" />

    <Button
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text=" Google "
        android:id="@+id/google"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true"
        android:layout_marginBottom="74dp"
        android:layout_marginTop="150dp"
        android:layout_marginLeft="15dp"
        android:background="#ffff2cca"
        android:textColor="#fff7fffc"
        android:textSize="25dp"
        android:textStyle="bold" />

    <Button
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text=" facebook "
        android:id="@+id/fb"
        android:layout_alignTop="@+id/google"
        android:layout_centerHorizontal="true"
        android:layout_marginLeft="8dp"
        android:background="#ff0204ff"
        android:textColor="#fff7fffc"
        android:textSize="25dp"
        android:textStyle="bold" />

    <Button
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Instagram"
        android:id="@+id/instagram"
        android:layout_alignTop="@+id/fb"
        android:layout_alignParentRight="true"
        android:layout_alignParentEnd="true"
        android:background="#ffee37ff"
        android:textColor="#fff7fffc"
        android:textSize="25dp"
        android:layout_marginLeft="8dp"
        android:textStyle="bold" />

    <Button
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text=" Yahoo "
        android:id="@+id/yahoo"
        android:background="#ff08ff06"
        android:textColor="#fff7fffc"
        android:textSize="25dp"
        android:layout_marginTop="230dp"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true"
        android:layout_marginLeft="15dp"
        android:textStyle="bold" />

    <Button
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text=" Gmail "
        android:id="@+id/gmail"
        android:background="#ffff131c"
        android:textColor="#fff7fffc"
        android:textSize="25dp"
        android:layout_alignTop="@+id/yahoo"
        android:layout_alignLeft="@+id/fb"
        android:layout_alignStart="@+id/fb"
        android:layout_alignBottom="@+id/yahoo"
        android:layout_toLeftOf="@+id/instagram"
        android:layout_toStartOf="@+id/instagram"
        android:textStyle="bold" />

    <Button
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text=" Twitter"
        android:id="@+id/twitter"
        android:background="#ff13e1ff"
        android:textColor="#fff7fffc"
        android:textSize="25dp"
        android:layout_marginTop="230dp"
        android:layout_marginLeft="250dp"
        android:textStyle="bold" />

    <Button
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text=" Youtube "
        android:id="@+id/youtube"
        android:background="#ffff131c"
        android:textColor="#fff7fffc"
        android:textSize="25dp"
        android:layout_marginTop="28dp"
        android:layout_below="@+id/yahoo"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true"
        android:layout_marginLeft="15dp"
        android:textStyle="bold" />

    <Button
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text=" Pinterest"
        android:id="@+id/pinterest"
        android:background="#ff0204ff"
        android:textColor="#fff7fffc"
        android:textSize="25dp"
        android:layout_alignTop="@+id/youtube"
        android:layout_toRightOf="@+id/youtube"
        android:layout_toEndOf="@+id/youtube"
        android:layout_marginLeft="8dp"
        android:textStyle="bold" />

    <Button
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Shubham"
        android:id="@+id/shubham"
        android:background="#ffff2cca"
        android:textColor="#fff7fffc"
        android:textSize="25dp"
        android:layout_toEndOf="@+id/pinterest"
        android:layout_alignTop="@+id/pinterest"
        android:layout_toRightOf="@+id/pinterest"
        android:layout_marginLeft="8dp"
        android:textStyle="bold" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textAppearance="?android:attr/textAppearanceLarge"
        android:text="Developed By:-"
        android:id="@+id/textView"
        android:layout_below="@+id/youtube"
        android:layout_centerHorizontal="true"
        android:layout_marginTop="55dp" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textAppearance="?android:attr/textAppearanceLarge"
        android:text="Shubham Gupta"
        android:id="@+id/textView2"
        android:layout_below="@+id/textView"
        android:layout_centerHorizontal="true"
        android:textColor="#ff0204ff"
        android:textSize="40dp"
        android:textStyle="bold" />

    <Button
        style="?android:attr/buttonStyleSmall"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Home"
        android:id="@+id/home"
        android:layout_marginLeft="140dp"
        android:layout_marginTop="490dp"
        android:textColor="#fff7fffc"
        android:textStyle="bold"
        android:background="#ffff131c"
        android:textSize="25dp"

        />


</RelativeLayout>
